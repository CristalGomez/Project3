  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "mongoose": "^5.5.6",
    "morgan": "^1.9.1",
    "react-stripe-elements": "^3.0.0",
    "stripe": "^6.34.0"
  },
  "devDependencies": {}
}
